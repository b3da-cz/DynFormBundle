{% extends 'b3daDynFormBundle::base.html.twig' %}

{% block body %}
    <div class="panel panel-default">
        <div class="panel-body">
            <h3>{{ 'dynForm.heading.formFieldCreate' | trans }}</h3>
        </div>
        <div class="panel-body">
            {{ form(form) }}
        </div>
        <div class="panel-body">
            <div class="form-group">
                <button id="dynForm_choiceAdd" class="btn btn-default">
                    {{ 'dynForm.buttonChoice.add' | trans }}
                </button>
            </div>
            <div id="dynForm_choiceList"></div>
        </div>
    </div>
{% endblock %}